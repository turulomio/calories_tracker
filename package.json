{
  "name": "calories_tracker",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "translate": "vue-i18n-extract --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locales/**/*.json' --add true --remove true --separator '###' ",
    "eslint": "eslint . --fix --ignore-pattern .gitignore --ignore-pattern output --ignore-pattern dist",
    "reusing": "node src/scripts/reusing.js",
    "vitest": "vitest --passWithNoTests",
    "playwright": "playwright test --ui --output output/playwright",
    "playwright_console": "playwright test --output output/playwright",
    "playwright_coverage": "nyc --reporter=lcov --report-dir  output/coverage  playwright test"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@tiptap/extension-color": "^3.14.0",
    "@tiptap/extension-mention": "^3.14.0",
    "@tiptap/extension-text-style": "^3.14.0",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "@tiptap/suggestion": "^3.14.0",
    "@tiptap/vue-3": "^3.14.0",
    "axios": "^1.13.2",
    "echarts": "^6.0.0",
    "flag-icons": "^7.5.0",
    "fraction.js": "^5.3.4",
    "html-to-pdfmake": "^2.5.32",
    "pdfmake": "^0.3.0-beta.8",
    "pinia": "^3.0.4",
    "roboto-fontface": "*",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.7",
    "vue-router": "^4.6.4",
    "vuetify": "^3.11.6",
    "vuetify_rules": "^0.7.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "npm-check-updates": "^19.2.0",
    "nyc": "^17.1.0",
    "playwright": "^1.57.0",
    "vite": "^7.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^4.0.16",
    "vue-i18n-extract": "github:Spittal/vue-i18n-extract"
  }
